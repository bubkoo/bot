{
  "name": "bot",
  "version": "1.3.17",
  "private": true,
  "description": "A GitHub App for me",
  "scripts": {
    "clean": "rimraf lib",
    "lint": "eslint 'src/**/*.{js,ts}?(x)' --fix",
    "build": "tsc",
    "build:watch": "tsc -w",
    "dev": "nodemon --exec \"npm start\"",
    "start": "probot run ./lib/index.js",
    "prebuild": "run-s clean",
    "prepare": "is-ci || husky install .husky"
  },
  "lint-staged": {
    "**/*.{js,jsx,tsx,ts,less,md,json}": [
      "pretty-quick â€”-staged"
    ],
    "*.ts": [
      "eslint --fix"
    ]
  },
  "author": {
    "name": "bubkooo",
    "email": "bubkoo.wy@gmail.com"
  },
  "license": "MIT",
  "dependencies": {
    "@wow-actions/app-token": "^1.1.13",
    "gemoji": "6.1.0",
    "js-yaml": "^4.1.0",
    "lodash": "^4.17.20",
    "minimatch": "^6.2.0",
    "mongoose": "^6.9.1",
    "probot": "^12.3.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@bubkoo/commitlint-config": "^1.0.2",
    "@bubkoo/eslint-config": "^1.4.2",
    "@bubkoo/tsconfig": "^1.0.0",
    "@types/js-yaml": "^4.0.3",
    "@types/lodash": "^4.14.189",
    "@types/minimatch": "^5.1.2",
    "@types/node": "^18.13.0",
    "@types/uuid": "^9.0.0",
    "eslint": "^8.34.0",
    "husky": "^8.0.3",
    "is-ci": "^3.0.1",
    "nock": "^13.3.0",
    "nodemon": "^2.0.4",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.4",
    "pretty-quick": "^3.1.3",
    "rimraf": "^4.1.2",
    "smee-client": "^1.1.0",
    "tslib": "^2.5.0",
    "typescript": "^4.9.5"
  },
  "engines": {
    "node": ">= 16"
  },
  "repository": "https://github.com/bubkoo/bot.git",
